{
    "version": 2,
    "builds": [
      {
        "src": "api/total_supply.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/totalsupply",
        "dest": "/api/total_supply.py"
      }
    ],
    "redirects": [
        { "source": "/index.html", "destination": "https://docs.isekailegends.com", "permanent": false },
        { "source": "/", "destination": "https://docs.isekailegends.com", "permanent": false }
      ]  
}
  